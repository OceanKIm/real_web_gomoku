<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ocean.realomuk.chat.ChatMapper">
	<insert id="insChat">
		INSERT INTO testchat (i_user, ctnt)
		VALUES (#{i_user}, #{ctnt})
	</insert>
	
	<select id="selChatList" resultType="ChatDomain">
		SELECT * FROM testchat
	</select>
	
	<delete id="delChatList">
		DELETE FROM testchat
	</delete>
</mapper>